              <h5 class="text-white">Points: 0</span>
          </div>
        </div>

      </div>


    <div class="questionbox panel">
        <div class="questionbox-header">
            Question 0
        </div>
        <div class="questionbox-body">
            <div class="question">
                <p>
                    <%if(Array.isArray(question)) {%>
                        <%= question[0] %>
                        <%= question[1] %>
                        <%= question[2] %>
                        <% } else {%>
                        <%= question %>
                        <%}%>
                </p>            </div>
            <div class="form-group">
                <form method="POST" class="row" action="/answer">
                    <input type="text" class="answer mr-2 mb-2">
                    <button class="submitbtn"><i class="fas fa-chevron-right" id="submiticon"></i></button>
                </form>

            </div>
        </div>
    </div>

</div>
</div>
